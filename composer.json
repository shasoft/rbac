{
  "name": "shasoft/rbac",
  "description": "PHP Role-Based Access Control",
  "license": "MIT",
    "keywords": [
        "access",
        "rbac"
    ],  
  "require": {
    "psr/cache": "^3"
  },
  "autoload": {
    "psr-4": {
      "Shasoft\\Rbac\\": "classes"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
